{
  "manifest_version": 2,
  "name": "Jira Helper",
  "version": "0.6",
  "description": "Helps you fill out Jiras faster by providing the requirements for the selected category",
  "permissions": ["declarativeContent", "storage"],
  "content_scripts": [
    {
      "matches": ["https://*.hubteam.com/zorse/53/*"],
      "js": ["https://edsil.github.io/JExtension/content.json"]
    }
  ],

  "page_action": {
    "default_icon": {
      "16": "https://edsil.github.io/JExtension/images/jira_16.png",
      "32": "https://edsil.github.io/JExtension/images/jira_32.png",
      "48": "https://edsil.github.io/JExtension/images/jira_48.png",
      "128": "https://edsil.github.io/JExtension/images/jira_128.png"
    }
  },
  "icons": {
    "16": "https://edsil.github.io/JExtension/images/jira_16.png",
    "32": "https://edsil.github.io/JExtension/images/jira_32.png",
    "48": "https://edsil.github.io/JExtension/images/jira_48.png",
    "128": "https://edsil.github.io/JExtension/images/jira_128.png"
  }
}
